            android:name="com.serc.projectspaceapp.NewTicket"
            android:exported="false" />
        <activity
            android:name="com.serc.projectspaceapp.Dashboard"
            android:exported="false" />
        <activity
            android:name="com.serc.projectspaceapp.SignUp"
            android:exported="false" />
        <activity
            android:name="com.serc.projectspaceapp.Login"
            android:exported="false" />
        <activity
            android:name="com.serc.projectspaceapp.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>